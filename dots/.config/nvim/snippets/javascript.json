{
  "const": {
    "prefix": "c",
    "body": "const ${1:name} = ${0:value}",
  },
  "let": {
    "prefix": "l",
    "body": "let ${1:name} = ${0:value}",
  },
  "export": {
    "prefix": "e",
    "body": "export ${0}",
  },
  "await": {
    "prefix": "aw",
    "body": "await ${0}",
  },
  "async": {
    "prefix": "as",
    "body": "async ${0}",
  },
  "debugger": {
    "prefix": "d",
    "body": "debugger",
  },
  "function": {
    "prefix": "f",
    "body": [
      "function ${1:name}(${2:params}) {",
      "\t${3}",
      "}"
    ],
  },
  "arrow function": {
    "prefix": "af",
    "body": "(${1:params}) => ${0:expression}",
  },
  "skeleton function": {
    "prefix": "sf",
    "body": [
      "function ${1:name}(${2:params}): ${3:void} {",
      "\t${0:throw new Error('Not implemented')}",
      "}"
    ],
  },
  "throw error": {
    "prefix": "tn",
    "body": "throw new Error(${0})",
  },
  "jsdoc": {
    "prefix": "jsdoc",
    "body": [
      "/**",
      " * ${0}",
      " */"
    ],
  },
  "block comment": {
    "prefix": "/",
    "body": "/* ${0} */",
  },
  "inline jsdoc": {
    "prefix": "/**",
    "body": "/** ${0} */",
  },
  "eslint disable next line": {
    "prefix": "eld",
    "body": "/* eslint-disable-next-line ${0} */",
  },
  "delay": {
    "prefix": "delay",
    "body": "await new Promise((resolve) => setTimeout(resolve, ${0:1000}))",
  }
}
