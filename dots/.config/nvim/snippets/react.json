{
  "react usestate": {
    "prefix": "us",
    "body": [
      "const [${1:state}, set${1/(.*)/${1:/capitalize}/}] = useState(${2})"
    ],
  },
  "react useeffect": {
    "prefix": "ue",
    "body": [
      "useEffect(() => {",
      "\t${1}",
      "}, [${0}])"
    ],
  },
  "html tag": {
    "prefix": "t",
    "body": [
      "<${1:div}>${0}</${1:div}>"
    ],
  },
  "class name": {
    "prefix": "cn",
    "body": "className=\"${0}\"",
  }
}
